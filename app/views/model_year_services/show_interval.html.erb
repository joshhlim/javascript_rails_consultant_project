<div class="row">
	<% if @model_year_service.interval %>
		<div class="col-md-4 col-md-offset-2">
			<%= render partial: 'entry', locals: { entry: @model_year_service.interval, list_header: "New Entry"  } %>
		</div>
	<% end %>
	
	<% if @model_year_service.interval.replaced %>
	  <div class="col-md-4">
	  	<%= render partial: 'entry', locals: { entry: @model_year_service.interval.replaced, list_header: "Old Entry"  } %>
	  </div>
	<% end %>
</div>
