<div id="add-to-garage-modal-window" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="add-to-garage-modal-label" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">Add to Garage</h4>
      </div>
      <div class="modal-body">
		<%= render 'user_vehicles/add_to_garage_form' %>
	</div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->